    body
        div(id='contact-modal' class='modal fade' tabindex='-1' role='dialog' aria-labelledby='contact-modal-label' aria-hidden='true')
            div(class='modal-dialog modal-dialog-centered' role='document')
                di(class='modal-content')
                    div(class='modal-header')
                        h5(id='contact-modal-label' class='modal-title') New message to Jared
                        button(type='button' class='close' data-dismiss='modal' aria-label='Close')
                            span(aria-hidden='true') &times;
                    div(class='modal-body')
                        form
                            div(class='form-group')
                                input(type='text' id='recipient-email' class='form-control' placeholder='Your Email Address')
                            div(class='form-group')
                                textarea(id='message-text' class='form-control' placeholder='Your Message')
                    div(class='modal-footer')
                        button(type='button' id='contact-modal-close' class='btn btn-cta-secondary' data-dismiss='modal') Close
                        button(type='button' id='contact-modal-send' class='btn btn-cta-primary') Send message
                    
        header(class='header')
            div(class='container clearfix')
                img(class='profile-image img-fluid float-left' src='/images/profile.png' alt='Jared Diehl')
                div(class='profile-content float-left')
                    h1(class='name') Jared Diehl
                    h2(class='desc') Application Software Developer
                    ul(class='social list-inline')
                        li(class='list-inline-item')
                            a(href='https://www.linkedin.com/in/jared-michael-diehl/')
                                i(class='fab fa-linkedin-in')
                        |                    
                        li(class='list-inline-item')
                            a(href='https://github.com/jaredible')
                                i(class='fab fa-github-alt')
                        |                    
                        li(class='list-inline-item')
                            a(href='https://www.facebook.com/Jaredible-569384850178383')
                                i(class='fab fa-facebook')
                        |                    
                        li(class='list-inline-item')
                            a(href='https://twitter.com/jaredmdiehl')
                                i(class='fab fa-twitter')
                        |                    
                        li(class='list-inline-item')
                            a(href='https://codepen.io/Jaredible')
                                i(class='fab fa-codepen')
                        |                    
                        li(class='list-inline-item')
                            a(href='https://stackoverflow.com/users/story/8322371')
                                i(class='fab fa-stack-overflow')
                        |                    
                        li(class='list-inline-item last-item')
                            a(href='https://www.youtube.com/channel/UCAUiywLH61_vV33R9n1hLvQ?view_as=subscriber')
                                i(class='fab fa-youtube')
                        |                    
                    |  
                button(type='button' class='btn btn-cta-primary float-right' data-toggle='modal' data-target='#contact-modal')
                    i(class='fas fa-paper-plane')
                    |  Contact Me
                |               

